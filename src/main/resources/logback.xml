<?xml version="1.0" encoding="UTF-8"?>
<!--Generated by ANT from config/template-files/impl/logback-application.properties-->
<configuration>
    <appender name="file" class="ch.qos.logback.core.FileAppender">
        <File>./interestmining-logger.log</File>
        <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
            <FileNamePattern>./interestmining-logger.log.%d{yyyy-MM-dd}.log</FileNamePattern>
        </rollingPolicy>
        <encoder>
            <pattern>%d{ISO8601} %p %c{1}:%L - %m%n</pattern>
        </encoder>
    </appender>
    <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
        <Target>System.out</Target>
        <encoder>
            <pattern>%d{HH:mm:ss.S} %-5level - %m%n</pattern>
        </encoder>
    </appender>

    <root level="info">
        <appender-ref ref="file"/>
        <appender-ref ref="stdout"/>
    </root>
</configuration>
